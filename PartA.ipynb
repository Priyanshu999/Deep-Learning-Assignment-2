        "def getInFeaturs(filter_sizes):\n",
        "  #layer1 output\n",
        "  in_features=224-filter_sizes[0]+1\n",
        "  in_features=(in_features-2)//2 + 1\n",
        "  for i in range(1, len(filter_sizes)):\n",
        "    in_features=in_features-filter_sizes[i]+1\n",
        "    in_features=(in_features-2)//2 + 1\n",
        "  return in_features\n"
